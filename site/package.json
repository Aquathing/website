{
  "name": "grammy-website",
  "version": "1.0.0",
  "description": "Main documentation site for grammY",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prepare": "npm run docs:index-versions",
    "docs:index-versions": "deno run --allow-net docs/.vuepress/plugins/current-versions/build-index.ts > docs/.vuepress/plugins/current-versions/index.json",
    "docs:dev": "vuepress-vite dev docs",
    "docs:build": "NODE_OPTIONS='--max-old-space-size=4096' npm run docs:vuepress && npm run docs:sitemap",
    "docs:vuepress": "vuepress-vite build docs",
    "docs:fmt": "deno fmt",
    "docs:sitemap": "sscli -b https://grammy.dev -r docs/.vuepress/dist --format xml --no-robots --no-clean"
  },
  "keywords": [],
  "author": "KnorpelSenf",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@grammyjs/types": "^2.9.1",
    "@vicons/fluent": "^0.12.0",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.51",
    "deno-bin": ">=1.25.2",
    "naive-ui": "^2.33.4",
    "static-sitemap-cli": "^2.1.2",
    "typescript": "^4.8.4",
    "vuepress-vite": "^2.0.0-beta.51"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "highlight.js": "^11.6.0",
    "grammy": "^1.12.4"
  }
}
